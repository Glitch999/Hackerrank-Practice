#include <iostream>

using namespace std;
/*
 * Create classes Rectangle and RectangleArea
 */
class Rectangle{
    public: 
    int width, height;
    void display()
    {
        cout << width <<" "<< height<<endl;
    }
};
class RectangleArea : public Rectangle
{
    public:
    int area;
    void read_input()
    {
        cin >> width >> height;
    }
    void display()
    {
        area = width * height;
        cout << area;
    }
};
